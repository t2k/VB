              <DbSource ConnectionRef="CreditNetConnectionString (Web.config)" DbObjectType="Unknown" GenerateMethods="Get" GenerateShortCommands="false" GeneratorGetMethodName="GetData" GetMethodModifier="Public" GetMethodName="GetData" QueryType="Rowset" ScalarCallRetval="System.Object, mscorlib, Version=4.0.0.0, Culture=neutral, PublicKeyToken=b77a5c561934e089" UseOptimisticConcurrency="false" UserGetMethodName="GetData" UserSourceName="GetData">
                <SelectCommand>
                  <DbCommand CommandType="Text" ModifiedByUser="true">
                    <CommandText>SELECT     SPRatings.Derived_Rating AS RatingSP, SUM(PortfolioTrades.NotionalAmount) / derivedtbl_1.PortTotal AS TOTAL, derivedtbl_1.PortTotal, 
                      SPRatings.Derived_Rating
FROM         Ratings INNER JOIN
                      SPRatings ON Ratings.Rating = SPRatings.Derived_Rating RIGHT OUTER JOIN
                      PortfolioTrades INNER JOIN
                      REDENTITY ON PortfolioTrades.TICKER = REDENTITY.TICKER ON SPRatings.CLIPS = REDENTITY.REDCODE CROSS JOIN
                          (SELECT     SUM(NotionalAmount) AS PortTotal
                            FROM          PortfolioTrades AS PortfolioTrades_1
                            WHERE      (Portfolio = @PORTFOLIO)) AS derivedtbl_1
WHERE     (PortfolioTrades.Portfolio = @PORTFOLIO) AND (Ratings.RAShort = N'SP' OR
                      Ratings.RAShort IS NULL)
GROUP BY Ratings.RatingOrder, derivedtbl_1.PortTotal, SPRatings.Derived_Rating
ORDER BY Ratings.RatingOrder</CommandText>
                    <Parameters>
                      <Parameter AllowDbNull="false" AutogeneratedName="PORTFOLIO" ColumnName="Portfolio" DataSourceName="CreditNet.dbo.PortfolioTrades" DataTypeServer="nvarchar(25)" DbType="String" Direction="Input" ParameterName="@PORTFOLIO" Precision="0" ProviderType="NVarChar" Scale="0" Size="25" SourceColumn="Portfolio" SourceColumnNullMapping="false" SourceVersion="Current" />
