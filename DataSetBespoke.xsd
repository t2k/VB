                <SelectCommand>
                  <DbCommand CommandType="Text" ModifiedByUser="true">
                    <CommandText>SELECT     ROUND(SUM(PortfolioTrades.NotionalAmount), 2) AS Notional, ROUND(SUM(PortfolioTrades.NotionalAmount * PortfolioTrades.DealRate) 
                      / SUM(PortfolioTrades.NotionalAmount), 2) AS waDealRate, ROUND(SUM(PortfolioTrades.NotionalAmount * mpCDSCurve._5Y * 10000) 
                      / SUM(PortfolioTrades.NotionalAmount), 2) AS waMktRate, spAssetType.IndustryName AS Industry
