          <TableAdapter BaseClass="System.ComponentModel.Component" DataAccessorModifier="AutoLayout, AnsiClass, Class, Public" DataAccessorName="PortfolioSPTableAdapter" GeneratorDataComponentClassName="PortfolioSPTableAdapter" Name="PortfolioSP" UserDataComponentName="PortfolioSPTableAdapter">
